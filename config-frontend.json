{
  "extends": ["tslint-react", "tslint-eslint-rules", "./config-base.json"],
  "rules": {
    "jsx-boolean-value": [true, "never"],
    "jsx-key": true,
    "jsx-no-bind": true,
    "jsx-no-lambda": true,
    "jsx-no-string-ref": true,
    "jsx-self-close": true,
    "no-constant-condition": true,
    "no-empty": [true, "allow-empty-catch"],
    "no-eval": true,
    "no-extra-semi": true,
    "no-ex-assign": false,
    "no-string-throw": true,
    "no-var-keyword": true,
    "prefer-const": true,
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "use-isnan": true,
    "valid-typeof": true
  }
}
